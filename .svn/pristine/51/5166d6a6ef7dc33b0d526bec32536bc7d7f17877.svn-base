			}

			.css {
				transition: width 2s;
				-moz-transition: width 2s;
				/* Firefox 4 */
				-webkit-transition: width 2s;
				/* Safari 和 Chrome */
				-o-transition: width 2s;
				/* Opera */
			}

			.slide_bg {
				background: #fd8c00;
				width: 100%;
				height: 100%;
			}

			.bg {
				width: 100vw;
				height: 100vh;
			}

			.tip {
				border-radius: 2vw;
				padding: 3%;
				color: #FFFFFF;
				text-align: left;
				opacity: 0;
				position: absolute;
				top: 48%;
				left: 20%;
				width: 54%;
				font-size: 4vw;
				background: rgba(0, 0, 0, 0.7);
			}

			.jiantou {
				position: absolute;
				top: 94%;
				opacity: 0;
				left: 47vw;
				width: 6vw;
				height: 6vw;
				background-image: url(http://alicdn.ldpro.cn/act0/img/shang.png);
				background-size: 100%;
			}

			.title {
				position: absolute;
				width: 100%;
				display: flex;
				justify-content: center;
				font-size: 4vw;
				align-items: center;
				top: 7%;
				font-weight: 500;
				font-family: "微软雅黑";
			}
		</style>
		<style></style>
	</head>
	<body>
		<!-- Swiper -->
		<div class="swiper-container swiper-container-vertical swiper-container-ios">
			<div class="swiper-wrapper" style="transform: translate3d(0px, 0px, 0px);">
				<div class="swiper-slide swiper-slide-active" style="height: 736px;">
					<div class="slide_bg">
						<div class="title">奖金如何申请提现 1/3</div>
						<div style="position:absolute;background: url(http://alicdn.ldpro.cn/act0/img/zhi.gif);background-size: 100%;width:15vw;top:75%;left:22vw;height:15vw;"></div>
						<img class="bg" src="http://alicdn.ldpro.cn/act0/img/guide10.min.png" style="width: 85vw;">
						<div class="tip" id="tip1" style="top: 45%; opacity: 1;">点击左侧「键盘图标」切换输入框</div>
						<div class="jiantou" style="top: 91.7667%; opacity: 0.744311;"></div>
					</div>
				</div>
				<div class="swiper-slide swiper-slide-next" style="height: 736px;">
					<div class="slide_bg">
						<div class="title">奖金如何申请提现 2/3</div>
						<div style="position:absolute;background: url(http://alicdn.ldpro.cn/act0/img/zhi.gif);background-size: 100%;width:15vw;top:50%;left:37vw;height:15vw;"></div>
						<img class="bg" src="http://alicdn.ldpro.cn/act0/img/guide11.min.png" style="width: 85vw;">
						<div class="tip" id="tip2" style="opacity: 0; top: 35%;">发送「提现」两个字给公众号</div>
						<div class="jiantou" style="top: 91.7667%; opacity: 0.744311;"></div>
					</div>
				</div>
				<div class="swiper-slide" style="height: 736px;">
					<div class="slide_bg">
						<div class="title">奖金如何申请提现 3/3</div>
						<div style="position:absolute;background: url(http://alicdn.ldpro.cn/act0/img/zhi.gif);background-size: 100%;width:15vw;top:59%;left:54vw;height:15vw;"></div>
						<img class="bg" src="http://alicdn.ldpro.cn/act0/img/guide12.min.png" style="width: 85vw;">
						<div class="tip" id="tip3" style="opacity: 0; top: 35%;">24小时内，系统将自动打款</div>
						<div class="jiantou" style="top: 91.7667%; opacity: 0.744311;"></div>
					</div>
				</div>

				<div class="swiper-slide" style="height: 736px;">
					<div class="slide_bg">
						<div class="tip_light"><img src="http://alicdn.ldpro.cn/act0/img/jc_light.png" style="width: 85vw;">猜你还想看</div>
						<div class="three_skip">
							<ul>
								<li>
									<img class="tubiao" src="http://alicdn.ldpro.cn/act0/img/jc_sq.png" style="width: 85vw;">
									<a href="cha.html">如何查淘宝隐藏券</a>
									<img class="jiantou2" src="http://alicdn.ldpro.cn/act0/img/jc_jiantou.png" style="width: 85vw;">
								</li>
								<li>
									<img class="tubiao" src="http://alicdn.ldpro.cn/act0/img/jc_jj.png" style="width: 85vw;">
									<a href="dan.html">付款后如何领奖金</a>
									<img class="jiantou2" src="http://alicdn.ldpro.cn/act0/img/jc_jiantou.png" style="width: 85vw;">
								</li>
							</ul>
						</div>
					</div>
				</div>
			</div>
			<!-- Add Pagination -->
			<div class="swiper-pagination swiper-pagination-clickable swiper-pagination-bullets"><span class="swiper-pagination-bullet swiper-pagination-bullet-active"
				 tabindex="0" role="button" aria-label="Go to slide 1"></span><span class="swiper-pagination-bullet" tabindex="0"
				 role="button" aria-label="Go to slide 2"></span><span class="swiper-pagination-bullet" tabindex="0" role="button"
				 aria-label="Go to slide 3"></span><span class="swiper-pagination-bullet" tabindex="0" role="button" aria-label="Go to slide 4"></span></div>
			<span class="swiper-notification" aria-live="assertive" aria-atomic="true"></span>
		</div>

		<!-- Swiper JS -->
		<script src="https://cdn.bootcss.com/Swiper/4.5.0/js/swiper.min.js"></script>
		<script src="https://cdn.bootcss.com/jquery/2.2.4/jquery.min.js"></script>
		<script>
			if (document.documentElement.clientHeight / document.documentElement.clientWidth < 1.8) {
				$('img').css('width', '85vw');
			}
			$('.jiantou').animate({
				top: '91%',
				opacity: '1'
			}, "1500");
			setInterval("jiantou()", 1500);

			function jiantou() {
				$('.jiantou').css('top', '94%');
				$('.jiantou').css('opacity', '0');
				$('.jiantou').animate({
					top: '91%',
					opacity: '1'
				}, "1500");
			}

			var swiper = new Swiper('.swiper-container', {
				direction: 'vertical',
				pagination: {
					el: '.swiper-pagination',
					clickable: true,
				},
				on: {
					slideChangeTransitionEnd: function() {
						switch (this.activeIndex) {
							case 0:
								layopen(0)
								break;
							case 1:
								layopen(1)
								break;
							case 2:
								layopen(2)
								break;
							case 3:
								layopen(3)
								break;
							case 4:
								layopen(4)
								break;
						}
					}
				}
			});
			layopen(0);

			function layopen(index) {
				if (index == 0) {
					back()
					$('#tip1').animate({
						top: '45%',
						opacity: '1'
					}, "2000");
				} else if (index == 1) {
					back()
					$('#tip2').animate({
						top: '32%',
						opacity: '1'
					}, "2000");
				} else if (index == 2) {
					back()
					$('#tip3').animate({
						top: '32%',
						opacity: '1'
					}, "2000");
				} else if (index == 3) {
					back()
					$('#tip4').animate({
						top: '55%',
						opacity: '1'
					}, "2000");
				} else if (index == 4) {
					back()
					$('#tip5').animate({
						top: '32%',
						opacity: '1'
					}, "2000");
				}
			}

			function back() {
				console.log(123)
				$('.tip').css('opacity', '0');
				$('.tip').css('top', '48%');
				$('#tip2').css('top', '35%');
				$('#tip3').css('top', '35%');
				$('#tip4').css('top', '58%');
				$('#tip5').css('top', '35%');
			}
		</script>
	</body>
</html>
