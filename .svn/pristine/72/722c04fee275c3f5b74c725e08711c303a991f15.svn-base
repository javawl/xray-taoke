		<script>
			var httpUrl = new lib.httpurl(location);
			(function() {
				var userId = document.getElementById("userId").value;
				var shopId = document.getElementById("shopId").value;
				var pageId = document.getElementById("pageId").value;

				function showErrorView(err) {
					document.getElementById('we-page').style.display = 'none';
					document.getElementById('error-body').className = 'error-content';
					if(err) {
						var errMsg = err.ret[0].substring(err.ret[0].indexOf('::') + 2);
						document.querySelector('#error-body .title').innerText = errMsg;
					}
				}
				var comboSearch = [];
				for(var i in httpUrl.params) {
					comboSearch.push(i + ":" + httpUrl.params[i]);
				}
				if((!userId && !shopId)) {
					showErrorView();
					return
				}
				lib.mtop.request({
					"api": "mtop.shop.render.getpageview",
					"v": "1.0",
					"ecode": 0,
					"data": {
						"userId": +userId,
						"shopId": +shopId,
						"pageId": +pageId,
						"pathInfo": "shop/index",
						"extendParams": comboSearch.join(";")
					}
				}, function(res) {
					var code = res.data.view;
					eval(code);
					var pageRender = require('@page/render');
					var container = document.getElementById('we-page');
					pageRender({
						container: container
					});
				}, function(err) {
					showErrorView(err);
				});
			}());
			document.getElementById('we-page').style.height = 'auto';
			document.getElementById('link').addEventListener('click', function(e) {
				var userId = document.getElementById("userId").value;
				userId && window.location.assign('//shop.m.taobao.com/shop/shopIndex.htm?user_id=' + userId + '#list');
			}, false);
		</script>
